@{
}

@model TableWithoutSyngonyModel

<div class="table-responsive">
    <table class="table table-sm">
        <thead>
            <tr>
                @foreach (string column in Model.ColumnNames)
                {
                    <th>@column</th>
                }
            </tr>
        </thead>
        <tbody>
            @foreach (var row in Model.Rows)
            {
                <tr>
                    @foreach (dynamic elem in row)
                    {
                        <td>@Html.Raw(elem)</td>
                    }
                </tr>
            }
        </tbody>
    </table>
</div>